@model IEnumerable<Nieruchomosci.Data.Data.Nieruchomosci.Nieruchomosc>
@{
    ViewData["Title"] = "Nieruchomosci";
    Layout = "~/Views/Shared/_NieruchomosciLayout.cshtml";
}
<!-- list-main-wrap-header end-->
<!-- listing-item-wrap-->
<div class="listing-item-container three-columns-grid  box-list_ic fl-wrap">
    <!-- listing-item -->
    @foreach (var item in Model)
    {
        <div class="listing-item">
            <article class="geodir-category-listing fl-wrap">
                <div class="geodir-category-img fl-wrap">
                    <a href="listing-single.html" class="geodir-category-img_item">
                        <img src="@item.Foto" alt="">
                        <div class="overlay"></div>
                    </a>
                    <div class="geodir-category-location">
                        <a href="#" class="single-map-item tolt" data-newlatitude="40.72956781" data-newlongitude="-73.99726866" data-microtip-position="top-left" data-tooltip="On the map"><i class="fas fa-map-marker-alt"></i> <span>@item.Adres</span></a>
                    </div>
                    <ul class="list-single-opt_header_cat">
                        <li><a href="#" class="cat-opt blue-bg">@item.Forma</a></li>
                        <li><a href="#" class="cat-opt color-bg">@item.RodzajNieruchomosci.Nazwa</a></li>
                    </ul>
                    <a href="#" class="geodir_save-btn tolt" data-microtip-position="left" data-tooltip=""><span><i class="fal fa-heart"></i></span></a>
                    <a href="#" class="compare-btn tolt" data-microtip-position="left" data-tooltip=""><span><i class="fal fa-random"></i></span></a>
                    <div class="geodir-category-listing_media-list">
                        <span><i class="fas fa-camera"></i></span>
                    </div>
                </div>
                <div class="geodir-category-content fl-wrap">
                    <h3 class="title-sin_item"><a href="listing-single.html">@Html.ActionLink(item.Tytul, "Szczegoly","Nieruchomosci", new {id=item.IdNieruchomosci}, null)</a></h3>
                    <div class="geodir-category-content_price">@item.Cena</div>
                    <div class="fw_hours fl-wrap"><span>@item.Opis</span> <br></div>
                    <div class="geodir-category-content-details">
                        <ul>
                            <li><i class="fal fa-bed"></i><span>@item.Sypialnia</span></li>
                            <li><i class="fal fa-bath"></i><span>@item.Lazienka</span></li>
                            <li><i class="fal fa-cube"></i><span>@item.Powierzchnia</span></li>
                        </ul>
                    </div>
                    <div class="geodir-category-footer fl-wrap">
                        <a href="" class="gcf-company"><span>Ocena</span></a>
                        <div class="listing-rating card-popup-rainingvis tolt" data-microtip-position="top" data-tooltip="Good" data-starrating2="@item.Ocena"></div>
                    </div>
                </div>
            </article>
        </div>
    }
    <!-- listing-item end-->
</div>
<!-- listing-item-wrap end-->
