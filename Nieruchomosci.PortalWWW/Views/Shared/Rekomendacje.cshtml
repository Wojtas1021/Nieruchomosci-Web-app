@model IEnumerable<Nieruchomosci.Data.Data.CMS.Rekomendacja>

<!-- section -->
<section class="gray-bg ">
    <div class="container">
        <div class="section-title st-center fl-wrap">
            <h4>Opinie </h4>
            <h2>Opinie Klientów</h2>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="testimonials-slider-wrap">
        <div class="testimonials-slider">
            <!--slick-item -->
            @foreach (var item in Model)
            {
                <div class="slick-item">
                    <div class="text-carousel-item fl-wrap">
                        <div class="text-carousel-item-header fl-wrap">
                            <div class="popup-avatar"><img src="images/avatar/1.jpg" alt=""></div>
                            <div class="review-owner fl-wrap">@item.Autor</div>
                            <div class="listing-rating card-popup-rainingvis" data-starrating2="@item.Ocena"> </div>
                        </div>
                        <div class="text-carousel-content fl-wrap">
                            <p>@item.Tresc</p>
                            <a href="#" class="testim-link color-bg">@item.Przycisk</a>
                        </div>
                    </div>
                </div>
            }
            <!--slick-item end -->
        </div>
    </div>
</section>
<!-- section end-->
